{
	"name": "ps/contact",
	"type": "typo3-cms-extension",
	"description": "TYPO3 contact extension based on tt_address",
	"keywords": [
		"TYPO3",
		"extension"
	],
	"authors": [
		{
			"name": "Christian Pschorr",
			"email": "pschorr.christian@gmail.com"
		}
	],
	"config": {
		"vendor-dir": "Vendor"
	},
	"require": {
		"typo3/cms-core": "^12.4",
		"jeroendesloovere/vcard": "dev-2.0.0-dev"
	},
	"autoload": {
		"psr-4": {
			"Ps\\Contact\\": "Classes",
			"JeroenDesloovere\\VCard\\": "Vendor/jeroendesloovere/vcard"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "contact"
		}
	}
}
